{
  "kind": "collectionType",
  "collectionName": "questions",
  "info": {
    "singularName": "question",
    "pluralName": "questions",
    "displayName": "Question",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "skill": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::skill.skill",
      "inversedBy": "questions"
    },
    "text": {
      "type": "text"
    },
    "answers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::answer.answer"
    },
    "Score": {
      "type": "integer"
    },
    "Category": {
      "type": "enumeration",
      "enum": [
        "easy",
        "medium",
        "hard",
        "legendary"
      ]
    },
    "quizzes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::quiz.quiz",
      "mappedBy": "questions"
    },
    "submission_questions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::submission-question.submission-question",
      "mappedBy": "question"
    }
  }
}
